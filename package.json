{
  "name": "planit-food",
  "version": "1.0.0",
  "description": "An app to manage the weekly food shop",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "watch": "webpack --watch --progress --profile",
    "test": "karma start",
    "lint": "tslint --force \"src/**/*.ts\"",
    "build:server": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "build": "npm run build:server && npm run build:client",
    "build:client": "rimraf client/public && webpack --progress --profile --bail",
    "start:server": "npm run build && node dist/index.js",
    "start:client": "webpack-dev-server --inline --progress --port 3000",
    "start": "npm run build:client && npm run start:server"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
